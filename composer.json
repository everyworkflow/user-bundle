{
    "name": "everyworkflow/user-bundle",
    "description": "User bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "5.4.*",
        "lexik/jwt-authentication-bundle": "^2.11",
        "symfony/security-bundle": "5.4.x",
        "everyworkflow/core-bundle": "dev-main",
        "everyworkflow/eav-bundle": "dev-main",
        "everyworkflow/data-grid-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\UserBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\UserBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
